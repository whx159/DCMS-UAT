<?xml version="1.0" encoding="UTF-8"?>

<!--  用于直接生成SQL语句，支持同步和异步提交 -->
<!--  title:标题
	navigate:导航信息
	type:方式（sql|call)
	interface:是否显示输入条件页面(true|false)
	databusid:数据总线编号
	transdataactionurl:数据整理URL
	txnlogflag:true记录日志
	async:异步提交标志
	pagesize:页面显示记录数(缺省为10)
-->
<CommQry title="SysParamsEntryDetail.title" navigate="系统维护  &gt; 系统参数设置" type="call"
         interface="true" pagesize="10" async="true" databusid=""
         transdataactionurl="/trans/TransDataAction.do"
         txnlogflag="true"
         getterclassname="com.huateng.report.system.getter.RoleMngCheckDetail">
    <Include id="BankParam"/>
    <!--
    <PageQryExp type="CSV" limit="true" encoding="UTF-8" url="/trans/qryExp.do" directory="F:/upload"/>
    -->
    <Fields>
        <!-- status :F-基本信息中显示  N-不显示 D-详细信息中显示 A-基本、详细信息中都显示  -->
        <!-- primary : true-作为表单项记录上传　false-不作为表单项上传 -->
        <Field id="taskId" desc="任务编号" datatype="string" size="10"
               readonly="true" xpath="/taskId"/>


        <Field id="old_roleid" desc="RoleMngCheckDetail.Field.old_roleid" datatype="string" size="10"
               readonly="true" xpath="/old_roleinfo/id"/>

        <Field id="old_roleName" desc="RoleMngCheckDetail.Field.old_roleName" datatype="string" size="10" readonly="true"
               xpath="/old_roleinfo/roleName"/>

        <Field id="old_status" desc="RoleMngCheckDetail.Field.old_status" status="N" readonly="true"
               translated="DATA_DIC.501" require="true" edittype="select" width="90"
               method="None" primary="true" type="String" size="20"
               xpath="/old_roleinfo/status" default="false" />

        <Field id="old_mail" desc="RoleMngCheckDetail.Field.old_mail" datatype="string" size="10" readonly="true"
               xpath="/old_roleinfo/mail"/>

        <Field id="new_roleid" desc="RoleMngCheckDetail.Field.new_roleid" datatype="string" size="10"
               readonly="true" xpath="/new_roleinfo/id"/>

        <Field id="new_roleName" desc="RoleMngCheckDetail.Field.new_roleName" datatype="string" size="10" readonly="true"
               xpath="/new_roleinfo/roleName"/>

        <Field id="new_status" desc="RoleMngCheckDetail.Field.new_status" status="N" readonly="true"
               translated="DATA_DIC.501" require="true" edittype="select" width="90"
               method="None" primary="true" type="String" size="20"
               xpath="/new_roleinfo/status" default="false" />

        <Field id="new_mail" desc="RoleMngCheckDetail.Field.new_mail" datatype="string" size="10" readonly="true"
               xpath="/new_roleinfo/mail"/>


    </Fields>

    <Operations>
        <Button id="btClose" name="btClose" desc="RoleMngCheckDetail.Operations.btClose" operation="syncsubmit"  icon="icon-back"
                url="#" txn=""/>
        <Button id="btApprove" name="btApprove" desc="RoleMngCheckDetail.Operations.btApprove" operation="asysubmit"
                updateClass="com.huateng.report.system.updater.RoleCheckApproveUpdater"  icon="icon-ok"
                url="#" txn=""/>
        <Button id="btReject" name="btReject" desc="RoleMngCheckDetail.Operations.btReject" operation="asysubmit"
                updateClass="com.huateng.report.system.updater.RoleCheckRejectUpdater" icon="icon-no"
                url="#" txn="" />
    </Operations>


</CommQry>
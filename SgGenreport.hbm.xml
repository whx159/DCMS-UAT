<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.huateng.hsbc.report.bean">
	<class name="SgGenreport" table="sggenreport">
		<id column="ID" name="id" type="string" />
		<property column="MERID" name="merId" type="string" />
		<property column="REPORT_NAME" name="reportName" type="string" />
		<property column="REPORT_FORMAT" name="reportFormat" type="string" />
		<property column="REPORT_DATE" name="reportDate" type="string" />
		<property column="STATUS" name="status" type="string" />
		<property column="COUNTRY" name="country" type="string" />
	</class>
</hibernate-mapping>
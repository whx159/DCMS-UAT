<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="resource.bean.hf">
	<class name="VNReconLog" table="VN_RECON_LOG">
		<id name="id" column="ID" type="string" />
		<property column="WORKDATE"    name="workdate"    type="string" />
		<property column="BATCH_NO"    name="batch_no"    type="string" />
		<property column="MERCHANT_ID"   name="merchant_id"   type="string" />
		<property column="ORDER_ID"    name="order_id"    type="string" />
		<property column="PAYMENT_DATE"  name="payment_date"  type="string" />
		<property column="PAYMENT_TIME"    name="payment_time"    type="string" />
		<property column="AMOUNT"    name="amount"    type="string" />
		<property column="COMMISSION_FEE"    name="commission_fee"    type="string" />
		<property column="REFUNDID"    name="refundid"    type="string" />
		<property column="ISSUE_DES"    name="issue_des"    type="string" />
	</class>
</hibernate-mapping>
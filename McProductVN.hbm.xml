<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="resource.bean.hf">
	<class name="McProductVN" table="MC_PRODUCT_VN">
		<id name="id" column="ID" type="string" />
		<property column="MERID"    name="merid"    type="string" />
		<property column="ITEM"    name="item"    type="string" />
		<property column="NAME"   name="name"   type="string" />
		<property column="ORDER_ID"    name="orderid"    type="string" />
		<property column="VALUE"  name="value"  type="string" />
		<property column="AGGREGATOR_ID"    name="aggregatorid"    type="string" />
	</class>
</hibernate-mapping>